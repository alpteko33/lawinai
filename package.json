{
  "name": "lawinai",
  "version": "1.0.0",
  "description": "AI-powered legal assistant for Turkish lawyers",
  "main": "main.js",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=production electron .",
    "dev": "concurrently \"npm run dev:vite\" \"wait-on http://localhost:5174 && cross-env NODE_ENV=development electron .\"",
    "dev:vite": "vite --port 5174",
    "build": "vite build",
    "dist": "npm run build && electron-builder",
    "pack": "electron-builder --dir",
    "postinstall": "electron-builder install-app-deps",
    "test:autocomplete": "node scripts/test-autocomplete.mjs",
    "test:plugin": "node scripts/test-plugin.mjs",
    "test:legal": "node scripts/test-legal-dict.mjs",
    "test": "vitest run",
    "test:unit": "vitest run"
  },
  "keywords": [
    "legal",
    "ai",
    "lawyer",
    "turkish",
    "assistant",
    "electron"
  ],
  "author": "LawInAI Team",
  "license": "MIT",
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "cross-env": "^10.0.0",
    "electron": "^28.1.0",
    "electron-builder": "^24.9.1",
    "jsdom": "^26.1.0",
    "vite": "^7.0.6",
    "vite-plugin-static-copy": "^3.1.1",
    "vitest": "^2.0.5",
    "wait-on": "^7.2.0"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@google/generative-ai": "^0.24.1",
    "@mui/icons-material": "^7.2.0",
    "@mui/material": "^7.2.0",
    "@nlux/react": "^2.17.1",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@reduxjs/toolkit": "^2.8.2",
    "@tailwindcss/typography": "^0.5.16",
    "@themaximalist/llm.js": "^1.0.1",
    "@tinymce/tinymce-react": "^6.2.1",
    "@tiptap/extension-bullet-list": "^3.0.7",
    "@tiptap/extension-color": "^3.0.7",
    "@tiptap/extension-highlight": "^3.0.7",
    "@tiptap/extension-list-item": "^3.0.7",
    "@tiptap/extension-ordered-list": "^3.0.7",
    "@tiptap/extension-placeholder": "^3.0.7",
    "@tiptap/extension-strike": "^3.0.7",
    "@tiptap/extension-subscript": "^3.0.7",
    "@tiptap/extension-superscript": "^3.0.7",
    "@tiptap/extension-table": "^3.0.7",
    "@tiptap/extension-table-cell": "^3.0.7",
    "@tiptap/extension-table-header": "^3.0.7",
    "@tiptap/extension-table-row": "^3.0.7",
    "@tiptap/extension-text-align": "^3.0.7",
    "@tiptap/extension-text-style": "^3.0.7",
    "@tiptap/extension-underline": "^3.0.7",
    "@tiptap/react": "^3.0.7",
    "@tiptap/starter-kit": "^3.0.7",
    "async-mutex": "^0.5.0",
    "autoprefixer": "^10.4.16",
    "chokidar": "^4.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "diff-match-patch": "^1.0.5",
    "electron-store": "^8.1.0",
    "lucide-react": "^0.300.0",
    "mammoth": "^1.10.0",
    "pdf-parse": "^1.1.1",
    "pdfjs-dist": "^5.3.93",
    "postcss": "^8.4.32",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-pdf": "^10.0.1",
    "react-redux": "^9.2.0",
    "react-resizable-panels": "^3.0.3",
    "redux": "^5.0.1",
    "redux-persist": "^6.0.0",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "tiff.js": "^1.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "build": {
    "appId": "com.lawinai.desktop",
    "productName": "LawInAI",
    "directories": {
      "output": "dist-electron"
    },
    "files": [
      "dist/**/*",
      "main.js",
      "preload.js",
      "package.json",
      "src/main/**/*"
    ],
    "mac": {
      "category": "public.app-category.productivity",
      "target": "dmg"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  }
}
